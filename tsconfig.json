{
  "extends": "astro/tsconfigs/strict",
  "include": [
    ".astro/types.d.ts",
    "**/*"
  ],
  "baseUrl": ".",
  "paths": {
    "@/layouts/*": [
      "src/layouts/*"
    ],
    "@components/*": [
      "src/components/*"
    ],
    "@/styles/*": [
      "src/styles/*"
    ],
    "@/markket/*": [
      "src/markket/*"
    ],
  },
  "exclude": [
    "dist"
  ],
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "react"
  }
}